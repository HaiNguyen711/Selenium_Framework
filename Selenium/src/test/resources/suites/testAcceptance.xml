<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="methods" thread-count="2">
	<!-- Supported browser names: Chrome; Firefox -->
	<parameter name="broswer" value="Chrome"></parameter>
	
	<!-- Value parameters: Local, Remote -->
	<parameter name="runningMode" value="Local"></parameter>
	<listeners>
		<listener class-name="core.report.TestListener"></listener>
	</listeners>
	
	<test name="Test">
		<classes>
			 <class name="tests.Agoda.LoginTest"></class>
		</classes>
	</test>
</suite>